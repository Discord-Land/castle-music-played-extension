{
    "name": "Castle Music",
    "version": "1.0",
    "description": "Build an Extension!",
    "manifest_version": 2,
    "background":{
        "scripts":["script.js"],
        "persistent": false
    },
    "page_action": {
        "default_popup": "popup.html"
    },
    "content_scripts": [
        {
          "matches": ["https://castle-music.site/*"],
          "js": [ "axios.js" , "index.js"]
        }
      ]
  }